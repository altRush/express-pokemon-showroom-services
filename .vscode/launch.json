	"version": "0.2.0",
	"configurations": [
		{
			"name": "Launch index.ts",
			"type": "node",
			"request": "launch",
			"runtimeArgs": ["-r", "ts-node/register"],
			"args": ["${workspaceFolder}/src/index.ts"]
		},
		{
			"name": "Attach to Process",
			"type": "node",
			"request": "attach",
			"restart": true,
			"port": 5858,
			"outFiles": [],
			"sourceMaps": true
		}
	]
}
